# description	: Wine's built-in replacement for Microsoft's .NET Framework
# depends	: wine
name=wine-mono
version=8.0.0
release=1
options="!strip"
source="https://github.com/madewokherd/wine-mono/releases/download/${name}-${version}/${name}-${version}-x86.msi"

build() {
	install -Dm644 wine-mono-$version-x86.msi $PKG/usr/share/wine/mono/wine-mono-$version.msi
}
