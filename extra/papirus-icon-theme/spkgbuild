# description	: Papirus icon theme
# depends	: gtk3

name=papirus-icon-theme
version=20171124
release=1
source=($name-$version.tar.gz::https://github.com/PapirusDevelopmentTeam/$name/archive/$version.tar.gz)

build() {
	cd $name-$version
	make DESTDIR=$PKG install
}

