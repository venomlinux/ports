# description	: DCFL (DoD Computer Forensics Lab) dd replacement with hashing
# depends	: glibc

name=dcfldd
version=1.3.4.1
release=1
source=(https://downloads.sourceforge.net/sourceforge/$name/$name-${version%.*}-${version##*.}.tar.gz)

build() {
	cd $name-${version%.*}-${version##*.}
	./configure --prefix=/usr --mandir=/usr/share/man
	make
	make DESTDIR=$PKG install
}

md5sum=(952026c872f11b53ce0ec6681a3eef0a)
