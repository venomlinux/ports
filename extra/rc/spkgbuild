# description	: BSD-style init (CRUX's init inspired)
# backup	: etc/rc.conf etc/rc.local
# depends	: sysvinit

name=rc
version=20180826
release=1
options=(!strip)
_commit=7258469d87898c9573ff84a383bf3264b1b93efc
source=($name-$_commit.tar.gz::https://github.com/emmett1/rc/archive/$_commit.tar.gz)
md5sum=(760b176a0212fc6236554a015f6e5e92)

build() {
	cd $name-$_commit
	make DESTDIR=$PKG install
	
}
